@import java.util.HashMap

pool: HashMap<Integer,String> %HMAP% 1
pool: Integer %INT% 3
pool: String %STRING% 3
pool: Character %CHAR% 3

property: %INT% == null || %INT% >= 0

%INT% := %[1..20]%
%CHAR% := (char) %INT%.intValue()
%STRING% := (%CHAR%).toString()
%HMAP% := new HashMap<Integer,String>()

%HMAP%.put(%INT%,%STRING%)
%HMAP,1%.containsKey(%INT,1%) -> %STRING% := %HMAP%.get(%INT%);
%HMAP%.clear()
@import java.util.HashMap

pool: HashMap<Integer,String> %HMAP% 2
pool: Integer %INT% 2
pool: String %STRING% 2
pool: Character %CHAR% 2

property: %INT% == null || %INT% >= 0

%INT% := %[48..90]%
%CHAR% := (char) %INT%.intValue()
%STRING% := (%CHAR%) + "s"
%HMAP% := new HashMap<Integer,String>()

%HMAP%.put(%INT%,%STRING%)
%HMAP,1%.containsKey(%INT,1%) -> %STRING% := %HMAP%.get(%INT%);
%HMAP%.clear()